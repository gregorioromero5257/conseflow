 */



var filter = __webpack_require__(231);
var every = __webpack_require__(275);
var unique = __webpack_require__(277);
var slice = __webpack_require__(278);
var indexOf = __webpack_require__(279);

module.exports = function intersection(arr) {
  if (arr == null) {
    return [];
  }

  if (arguments.length === 1) {
    return unique(arr);
  }

  var arrays = slice(arguments, 1);

  return filter(unique(arr), function (ele) {
    return every(arrays, function (cur) {
      return indexOf(cur, ele) !== -1;
    });
  });
};


